{
  "$schema": "https://json-schema.org/schema",
  "version": 2,
  "title": "DockerCompose executor",
  "description": "",
  "type": "object",
  "properties": {
    "projectFile": {
      "type": "string",
      "description": "Path to the docker-compose file"
    },
    "command": {
      "type": "string",
      "description": "Command to run",
      "default": "up",
      "enum": ["up", "down"]
    }
  },
  "required": ["projectFile"],
}
